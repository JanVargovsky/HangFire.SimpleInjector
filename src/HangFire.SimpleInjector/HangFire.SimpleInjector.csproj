<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net45</TargetFramework>
    <AssemblyTitle>Hangfire.SimpleInjector</AssemblyTitle>
    <Company>DevMondo</Company>
